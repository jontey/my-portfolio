{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "Portfolio using Webpack + React.js",
  "main": "js/scripts.js",
  "scripts": {
    "build":
      "webpack --config webpack/webpack.prod.config.js --progress --colors",
    "watch": "webpack --config webpack/webpack.prod.config.js --watch",
    "start": "node webpack/server.js",
    "precommit": "lint-staged",
    "test": "npm run build"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jontey/my-portfolio.git"
  },
  "homepage": "https://github.com/jontey/my-portfolio#readme",
  "license": "MIT",
  "devDependencies": {
    "react-hot-loader": "^3.0.0",
    "style-loader": "^0.19.0",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-brands": "^5.0.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "0.0.17",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "greenkeeper-lockfile": "^1.14.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "mini.css": "^2.3.7",
    "node-sass": "^4.5.3",
    "prettier": "^1.11.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "reactstrap": "^5.0.0-alpha.4",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0"
  }
}
